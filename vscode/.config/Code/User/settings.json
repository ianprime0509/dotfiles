{
    "editor.rulers": [
        80
    ],
    "keyboard.dispatch": "keyCode",
    "[json]": {
        "editor.tabSize": 2
    },
    "[jsonc]": {
        "editor.tabSize": 2
    },
    "[html]": {
        "editor.tabSize": 2
    },
    "[css]": {
        "editor.tabSize": 2
    },
    "[javascript]": {
        "editor.tabSize": 2
    },
    "[tex]": {
        "editor.tabSize": 2
    },
    "[latex]": {
        "editor.tabSize": 2
    },
    "[makefile]": {
        "editor.insertSpaces": false,
        "editor.tabSize": 8
    },
    "editor.renderWhitespace": "boundary",
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "files.insertFinalNewline": true,
    "files.associations": {
        "*.tpp": "cpp"
    },
    "latex-workshop.view.pdf.viewer": "tab"
}
