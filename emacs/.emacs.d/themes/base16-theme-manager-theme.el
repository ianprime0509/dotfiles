;;; base16-theme-manager-theme.el --- Theme-manager generated theme (Base16 theme)

(require 'base16-theme)

(defvar base16-theme-manager-colors
  '(:base00 "#21282C"
    :base01 "#364349"
    :base02 "#4C5D66"
    :base03 "#627884"
    :base04 "#7B919D"
    :base05 "#99AAB3"
    :base06 "#B6C2C9"
    :base07 "#6B8390"
    :base08 "#996B5C"
    :base09 "#E2A488"
    :base0A "#188499"
    :base0B "#537A99"
    :base0C "#7A9DCB"
    :base0D "#705C99"
    :base0E "#E3729D"
    :base0F "#E15D75")
  "Colors for Theme-manager generated theme")

;; Define the theme
(deftheme base16-theme-manager)

;; Add all the faces to the theme
(base16-theme-define 'base16-theme-manager base16-theme-manager-colors)

;; Mark the theme as provided
(provide-theme 'base16-theme-manager)

(provide 'base16-theme-manager-theme)

;;; base16-theme-manager-theme.el ends here
